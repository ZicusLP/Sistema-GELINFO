<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>GELINFO</display-name>
	<welcome-file-list>
		<welcome-file>includes/login.jsp</welcome-file>
		
	</welcome-file-list>
	<servlet>
		<servlet-name>Faces Servlet</servlet-name>
		<servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Faces Servlet</servlet-name>
		<url-pattern>/faces/*</url-pattern>
		<url-pattern>*.xhtml</url-pattern>
	</servlet-mapping>



	<context-param>
		<param-name>primefaces.THEME</param-name>
		<param-value>humanity</param-value>
	</context-param>
	
	<error-page>
	<exception-type>
	br.com.gelinfo.exception.InvalidUserException
	</exception-type>
	<location>
	/includes/errodeusuario.jsp
	</location>
	</error-page>
	
	<mime-mapping>
   <extension>jsp</extension>
   <mime-type>application/jsp</mime-type>
</mime-mapping> 

</web-app>